{
    "version": 2,
    "builds": [
        {
            "src": "finance_tracker/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "finance_tracker/wsgi.py"
        }
    ],
    "env": {
        "VERCEL": "1"
    }
}