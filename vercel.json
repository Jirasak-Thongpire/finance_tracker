{
    "version": 2,
    "builds": [
        {
            "src": "finance_tracker/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "finance_tracker/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "finance_tracker.settings"
    }
}